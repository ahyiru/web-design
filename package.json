{
  "name": "web-design",
  "version": "1.8.0",
  "type": "module",
  "description": "Web Design 是一个简单的低代码开发平台，用于快速搭建项目、开发页面、配置接口等。",
  "keywords": [
    "react",
    "low-code",
    "web-design",
    "scene-design",
    "json schema",
    "web3",
    "webgl",
    "webpack",
    "full-stack",
    "MERN",
    "customizable",
    "responsive",
    "admin",
    "template",
    "chatgpt",
    "AIGC",
    "llm",
    "langchain"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "postinstall": "husky install",
    "start": "pack start",
    "build": "pack run build",
    "analyze": "pack run analyze",
    "server": "pack run server",
    "test": "pack test",
    "release": "pack release",
    "eslint": "pack eslint 'app/**/*.{js,jsx}'",
    "stylelint": "pack stylelint 'app/**/*.{css,less}'",
    "lint": "npm run eslint && npm run stylelint",
    "lint-fix": "pack eslint --fix 'app/**/*.{js,jsx}' && pack stylelint --fix 'app/**/*.{css,less}'",
    "prettier": "pack prettier 'app/**/*' --write --ignore-unknown && npm run lint-fix",
    "server-fix": "pack eslint --fix 'server/**/*.{js,jsx,mjs}'",
    "server-pre": "pack prettier 'server/**/*' --write --ignore-unknown && npm run server-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/web-design"
  },
  "license": "MIT",
  "devDependencies": {
    "@huxy/pack": "^1.6.6",
    "husky": "^9.1.0",
    "webpack-deadcode-plugin": "^0.1.17"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@ant-design/pro-table": "^3.16.5",
    "@huxy/components": "^2.1.12",
    "@huxy/layout": "^2.1.8",
    "@huxy/materials": "^1.1.4",
    "@huxy/router": "^2.1.9",
    "@huxy/use": "^2.1.7",
    "@huxy/utils": "^2.1.13",
    "antd": "^5.19.2",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "highlight.js": "^11.10.0",
    "html2canvas": "^1.4.1",
    "marked": "^13.0.2",
    "marked-highlight": "^2.1.3",
    "qrcode": "^1.5.3",
    "react": "19.0.0-rc-512b09b2-20240718",
    "react-dom": "19.0.0-rc-512b09b2-20240718",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "react-window-infinite-loader": "^1.0.9",
    "slate": "^0.103.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.107.1",
    "socket.io-client": "^4.7.5",
    "three": "^0.166.1",
    "web3": "^4.11.0"
  }
}
