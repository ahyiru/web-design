{
  "name": "web-design",
  "version": "1.5.0",
  "description": "Low-code development platform. Built with React18, Webpack5, useRouter, useStore, eslint, prettier, husky, commitlint, etc.",
  "keywords": [
    "React18",
    "useRouter",
    "useStore",
    "layout",
    "low-code",
    "no-code",
    "web-design",
    "scene-design",
    "web3",
    "webgl",
    "hooks",
    "Webpack5",
    "eslint",
    "prettier",
    "husky",
    "commitlint",
    "admin",
    "template",
    "dashboard"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "postinstall": "husky install",
    "start": "pack start",
    "build": "pack run build",
    "analyze": "pack run analyze",
    "server": "pack run server",
    "test": "pack run test",
    "release": "pack run release",
    "eslint": "eslint 'app/**/*.{js,jsx}'",
    "stylelint": "stylelint 'app/**/*.{css,less}'",
    "lint": "npm run eslint && npm run stylelint",
    "lint-fix": "eslint --fix 'app/**/*.{js,jsx}' && stylelint --fix 'app/**/*.{css,less}'",
    "prettier": "prettier 'app/**/*' --write --ignore-unknown && npm run lint-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/web-design"
  },
  "license": "MIT",
  "devDependencies": {
    "@huxy/pack": "^0.5.0",
    "husky": "^8.0.3"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-table": "^3.3.1",
    "@antv/graphin": "^2.7.13",
    "@antv/graphin-components": "^2.4.0",
    "@antv/x6": "^2.5.1",
    "@antv/x6-react-components": "^2.0.7",
    "@antv/x6-react-shape": "^2.1.0",
    "@huxy/components": "^1.9.6",
    "@huxy/layout": "^1.9.4",
    "@huxy/materials": "^0.2.1",
    "@huxy/router": "^1.9.2",
    "@huxy/use": "^1.9.1",
    "@huxy/utils": "^1.9.2",
    "antd": "^5.2.3",
    "echarts": "^5.4.1",
    "echarts-for-react": "^3.0.2",
    "highlight.js": "^11.7.0",
    "html2canvas": "^1.4.1",
    "marked": "^4.2.12",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.8",
    "rxjs": "^7.8.0",
    "slate": "^0.91.4",
    "slate-history": "^0.86.0",
    "slate-react": "^0.91.7",
    "three": "^0.150.1",
    "web3": "^1.8.2"
  }
}
