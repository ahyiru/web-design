{
  "name": "web-design",
  "version": "1.6.0",
  "type": "module",
  "description": "Low-code development platform. 使用界面可视化操作来配置生成页面描述 json schema，然后通过自定义规则将 json schema 渲染成我们需要的页面。",
  "keywords": [
    "react",
    "low-code",
    "web-design",
    "scene-design",
    "json schema",
    "useRouter",
    "useStore",
    "chatgpt",
    "huxy",
    "web3",
    "webgl",
    "hooks",
    "webpack",
    "eslint",
    "prettier",
    "husky",
    "commitlint",
    "admin",
    "template",
    "dashboard"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "postinstall": "husky install",
    "start": "pack start",
    "build": "pack run build",
    "analyze": "pack run analyze",
    "server": "pack run server",
    "test": "pack run test",
    "release": "pack run release",
    "eslint": "eslint 'app/**/*.{js,jsx}'",
    "stylelint": "stylelint 'app/**/*.{css,less}'",
    "lint": "npm run eslint && npm run stylelint",
    "lint-fix": "eslint --fix 'app/**/*.{js,jsx}' && stylelint --fix 'app/**/*.{css,less}'",
    "prettier": "prettier 'app/**/*' --write --ignore-unknown && npm run lint-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/web-design"
  },
  "license": "MIT",
  "devDependencies": {
    "@huxy/pack": "^1.1.5",
    "husky": "^8.0.3"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-table": "^3.6.1",
    "@antv/graphin": "^2.7.13",
    "@antv/graphin-components": "^2.4.0",
    "@antv/x6": "^2.9.4",
    "@antv/x6-react-components": "^2.0.7",
    "@antv/x6-react-shape": "^2.1.1",
    "@huxy/components": "^2.0.4",
    "@huxy/layout": "^2.0.2",
    "@huxy/materials": "^1.0.2",
    "@huxy/router": "^2.0.1",
    "@huxy/use": "^2.0.1",
    "@huxy/utils": "^2.0.4",
    "antd": "^5.4.0",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "highlight.js": "^11.7.0",
    "html2canvas": "^1.4.1",
    "marked": "^4.3.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.11",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.8",
    "rxjs": "^7.8.0",
    "slate": "^0.93.0",
    "slate-history": "^0.93.0",
    "slate-react": "^0.93.0",
    "three": "^0.151.3",
    "web3": "^1.9.0"
  }
}
